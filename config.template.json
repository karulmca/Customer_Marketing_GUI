{
    "scraper_settings": {
        "delay_range": [10, 20],
        "timeout": 30,
        "max_retries": 3,
        "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36"
    },
    "openai_settings": {
        "api_key": "your-openai-api-key-here",
        "model": "gpt-3.5-turbo",
        "max_tokens": 150,
        "temperature": 0.1
    },
    "default_columns": {
        "url_column": "LinkedIn_URL",
        "size_column": "Company_Size"
    },
    "logging": {
        "level": "INFO",
        "file": "linkedin_scraper.log",
        "format": "%(asctime)s - %(levelname)s - %(message)s"
    },
    "excel_settings": {
        "backup_suffix": ".backup",
        "save_progress_interval": 10
    },
    "company_size_patterns": [
        "(\\d{1,3}(?:,\\d{3})*\\+?\\s*(?:-\\s*\\d{1,3}(?:,\\d{3})*\\+?)?\\s*employees?)",
        "((?:\\d{1,3}(?:,\\d{3})*\\+?)\\s*(?:-\\s*(?:\\d{1,3}(?:,\\d{3})*\\+?))?\\s*employees?)",
        "(Company size[:\\s]*\\d{1,3}(?:,\\d{3})*\\+?\\s*(?:-\\s*\\d{1,3}(?:,\\d{3})*\\+?)?\\s*employees?)"
    ],
    "linkedin_selectors": [
        "dd[class*=\"company-size\"]",
        "span[class*=\"company-size\"]",
        "div[class*=\"company-size\"]",
        ".org-top-card-summary-info-list__info-item",
        ".org-about-company-module__company-size-definition"
    ]
}